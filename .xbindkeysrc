
# Increase volume
"dwm-increasevolume"
   XF86AudioRaiseVolume

# Decrease volume
"dwm-decreasevolume"
   XF86AudioLowerVolume

# Mute volume
"dwm-togglemute"
   XF86AudioMute

# decrease brightness
"dwm-decreasebrightness"
   XF86MonBrightnessDown

# decrease backlight
"dwm-increasebrightness"
   XF86MonBrightnessUp

"dwm-takescreenshot"
   Print

"dwm-wifikill"
   XF86WLAN

"dmenu_run"
   XF86Search

"dwm-mutemic"
   XF86AudioMicMute

